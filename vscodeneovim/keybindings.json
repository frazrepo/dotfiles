// Place your key bindings in this file to override the defaultsauto[]
// Enable jj and jk for ESC
[
    {
        "key": "j",
        "command": "vscode-neovim.compositeEscape1",
        "when": "neovim.mode == insert",
        "args" : "j"
    },
    {
        "key": "k",
        "command": "vscode-neovim.compositeEscape2",
        "when": "neovim.mode == insert",
        "args" : "k"
    }
]
